#include <string>

using namespace std;

int main() {
	//10^5 = 100,000
	string s, temp;
	cin >> s;
	vector<int>v(s.size());
	int n(0);
	
	for (int i = 0; i < s.size(); i++) {
		temp = s[i];
		v[i] = stoi(temp);
	}
	sort(v.begin(), v.end(), greater<>());
	
	for (int i = 0; i < v.size(); i++) 
		if (v[i] != 0) n++;
	for (int i = 0; i < n; i++) {
		
	}
	

	return 0;
}
